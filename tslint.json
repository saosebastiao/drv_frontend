{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "jsx-no-multiline": false,
    "jsx-no-multiline-js": false,
    "jsx-no-string-ref": false,
    "jsx-alignment": false,
    "jsx-boolean-value": [
      true,
      "never"
    ],
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": true,
    "interface-name": false,
    "member-ordering": false,
    "max-classes-per-file": [
      true,
      2
    ],
    "new-parens": false,
    "arrow-parens": false,
    "ordered-imports": false,
    "one-line": [
      true,
      "check-catch",
      "check-finally",
      "check-else"
    ],
    "array-type": [
      true,
      "generic"
    ],
    "variable-name": false,
    "trailing-comma": false,
    "curly": [
      true,
      "ignore-same-line"
    ]
  },
  "rulesDirectory": []
}