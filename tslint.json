{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "jsx-no-multiline": false,
        "jsx-no-multiline-js": false,
        "jsx-boolean-value": [
            true,
            "never"
        ],
        "jsx-no-lambda": false,
        "object-literal-sort-keys": false,
        "no-unused-variable": true,
        "interface-name": false,
        "member-ordering": false,
        "max-classes-per-file": [
            true,
            2
        ],
        "one-line": [
            true,
            "check-catch",
            "check-finally",
            "check-else"
        ],
        "array-type": [
            true,
            "generic"
        ],
        "curly": [
            true,
            "ignore-same-line"
        ]
    },
    "rulesDirectory": []
}